{
  "name": "meter-core",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "exports": {
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,ts}\"",
    "build": "tsc --noEmit && tsup",
    "build:test": "tsup src/test.ts --clean --no-config",
    "test": "ts-node src/test.ts"
  },
  "dependencies": {
    "cap": "^0.2.1",
    "is-admin": "^3.0.0",
    "lz4-napi": "^2.2.0",
    "oodle": "github:Mathicha/oodle",
    "snappyjs": "^0.7.0",
    "tiny-typed-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.0",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "^18.11.11",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
